{
  "version": 2,

  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.11"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts",
      "headers": { "Access-Control-Allow-Origin": "*" },
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}