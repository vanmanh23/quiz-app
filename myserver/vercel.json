{
  "version": 2,
  "functions":{
    "index.ts": {
      "memory": 3009,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts",
      "methods": ["POST", "GET", "PUT", "DELETE", "OPTIONS", "HEAD"]
    }
  ],
  "redirects": [
    {"src": "/api/(.*)", "dest": "/", "status": 200}
  ]
}