{
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./api/index.js",
      "dest": "/(.*)",
      "headers": { "Access-Control-Allow-Origin": "*" },
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}