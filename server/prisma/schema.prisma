// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Image {
  id        String      @id 
  uri       String
  alt       String
  flagTest  FlagTest?  @relation(fields: [flagTestId], references: [id])
  flagTestId String?      @unique
}
model FlagTest {
  id              String       @id 
  title           String
  testName        String
  numOfQuestions  Int
  duration        Int
  image           Image?    
}